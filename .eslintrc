{
    "root": true,
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "parser": "babel-eslint"
    },
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "rules": {
        "no-undef": "warn",
        "eqeqeq": "warn",
        "multiline-ternary": ["warn", "always-multiline"]
      }
  }